{
  "spin_manifest_version": 2,
  "application": {
    "name": "update-component-case",
    "trigger": {
      "fake": {}
    }
  },
  "trigger": {
    "fake": [
      {
        "id": "trigger-snake-case",
        "component": "snake-case"
      },
      {
        "id": "trigger-mixedcase",
        "component": "mixedcase"
      }
    ]
  },
  "component": {
    "snake-case": {
      "source": "wasm.wasm",
      "allowed_outbound_hosts": [
        "redis://*:*",
        "mysql://*:*",
        "postgres://*:*"
      ]
    },
    "mixedcase": {
      "source": "wasm.wasm",
      "allowed_outbound_hosts": [
        "redis://*:*",
        "mysql://*:*",
        "postgres://*:*"
      ]
    }
  }
}